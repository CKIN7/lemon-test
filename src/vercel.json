{
    "version": 2,
    "builds": [
        {
            "src": "src/index.html",
            "use": "@vercel/static-build",
            "dest": "dist/index.html"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.html"
        }
    ],
    "public": "dist/"
}
